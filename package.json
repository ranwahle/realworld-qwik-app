{
  "name": "realworld-qwik-app",
  "description": "Blank Qwik starter app. Vite.js tooling. Prettier code formatter.",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "npm run build.client && vite build --ssr src/entry.cloudflare-pages.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "prepare": "husky install",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.10.0",
    "@builder.io/qwik-city": "0.0.111",
    "@types/eslint": "8.4.6",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "8.23.1",
    "eslint-plugin-qwik": "0.9.0",
    "husky": "^8.0.0",
    "prettier": "2.7.1",
    "typescript": "4.8.3",
    "vite": "3.1.1",
    "vite-tsconfig-paths": "3.5.0",
    "wrangler": "beta"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "axios": "^0.27.2"
  }
}
