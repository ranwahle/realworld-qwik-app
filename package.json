{
  "name": "realworld-qwik-app",
  "description": "Blank Qwik starter app. Vite.js tooling. Prettier code formatter.",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "build.server": "vite build --ssr src/entry.netlify-edge.tsx",
    "build.netlify": "vite build --ssr src/entry.netlify-edge.tsx ",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "prepare": "husky install",
    "preview": "qwik build preview && vite preview --open",
    "serve": "netlify dev",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.12.1",
    "@builder.io/qwik-city": "0.0.118",
    "@types/eslint": "8.4.8",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.26.0",
    "eslint-plugin-qwik": "0.12.1",
    "node-fetch": "3.2.10",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "vite": "3.2.1",
    "vite-tsconfig-paths": "3.5.0",
    "@netlify/vite-plugin-netlify-edge": "1.1.0",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "axios": "^0.27.2"
  }
}
